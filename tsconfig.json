{
  
  
 "compilerOptions": {
   "module": "commonjs",
   "esModuleInterop": true,
   "target": "es6",
   "moduleResolution": "node",
   "sourceMap": true,
   "outDir": "dist",
   "rootDir": "src",
   "declaration": true,
   "types": ["node"],
   "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
    }
 },
 "lib": ["es2015"],
 "include": ["src/"],
 "exclude": [
     "dist",
     "node_modules",
     "src/**/*.spec.ts"
   ],
}